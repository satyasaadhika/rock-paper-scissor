{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
      {
        "src": "/api/index.php",
        "dest": "/api/index.php"
      },
      {
        "src": "/login.php",
        "dest": "/login.php"
      },
      {
        "src": "/game.php",
        "dest": "/game.php"
      },
      {
        "src": "/bootstrap.php",
        "dest": "/bootstrap.php"
      },
      {
        "src": "/(.*).png",
        "dest": "/$1.png"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
}
